              "color" : "int",
              "mw" : "int"
            },
            "5": {
              "color" : "int",
              "mw" : "int"
            },
            "6": {
              "color" : "int",
              "mw" : "int"
            },
            "7": {
              "color" : "int",
              "mw" : "int"
            },
            "8": {
              "color" : "int",
              "mw" : "int"
            },
            "9": {
              "color" : "int",
              "mw" : "int"
            },
            "10": {
              "color" : "int",
              "mw" : "int"
            },
            "11": {
              "color" : "int",
              "mw" : "int"
            },
            "12": {
              "color" : "int",
              "mw" : "int"
            },
            "13": {
              "color" : "int",
              "mw" : "int"
            },
            "14": {
              "color" : "int",
              "mw" : "int"
            },
            "15": {
              "color" : "int",
              "mw" : "int"
            }
          }
        }
      },
      "spectrum" : {
        "version" : 1,
        "preload" : false,
        "struct" : {
          "channel" : "int",
          "wave" : "int",
          "value" : "int"
        }
      },
      "dots" : {
        "version" : 1,
        "migration" : "migration",
        "struct" : {
          "time" : "int",
          "brightness" : "double",
          "spectrum" : {
            "0": "int",
            "1": "int",
            "2": "int",
            "3": "int",
            "4": "int",
            "5": "int",
            "6": "int",
            "7": "int",
            "8": "int",
            "9": "int",
            "10": "int",
            "11": "int",
            "12": "int",
            "13": "int",
            "14": "int",
            "15": "int"
          }
        }
      }
    }
  },
  "requires" : {
    "interfaces" : {
      "one_wire" : {
        "type" : "1wire",
        "default" : [16, 17],
        "description" : {
          "ru" : "Датчики 1wire",
